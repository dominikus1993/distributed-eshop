{
    "name": "ShoppingListStorage",
    "dockerComposeFile": ["../docker-compose.dev.yml"],
    "service": "storage",
    "shutdownAction": "none",
    "extensions": ["golang.Go", "wakatime.vscode-wakatime", "GitHub.copilot"],
    "settings": {
		"go.toolsManagement.checkForUpdates": "local",
		"go.useLanguageServer": true,
		"go.gopath": "/go",
		"go.goroot": "/usr/local/go"
	},
    "forwardPorts": [9000],
    "workspaceFolder": "/workspace/shopping-list-storage"
  }